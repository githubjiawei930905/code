package com.epeibao.po;

import java.util.Date;

/**
 * 车辆保养信息实体类
 * 
 * @author pk
 * 
 */
public class VehicleMaintenance {

	private int vehicleMaintenanceID;//ID
	private String comprehensiveAssessmentID;//综合测评ID
	private String busNumber;// 车牌号
	private int phototype; // 照片类型
	private String photoPath; // 照片数据
	private int ifAssessment; // 是否审核通过
	private Date assessmentTime; // 审核时间
	private String cause; //原因
	
	

	public String getCause() {
		return cause;
	}
	public void setCause(String cause) {
		this.cause = cause;
	}
	public VehicleMaintenance() {}
	public VehicleMaintenance(
			String comprehensiveAssessmentID, String busNumber, int phototype,
			String photoPath, int ifAssessment, Date assessmentTime) {
		this.comprehensiveAssessmentID = comprehensiveAssessmentID;
		this.busNumber = busNumber;
		this.phototype = phototype;
		this.photoPath = photoPath;
		this.ifAssessment = ifAssessment;
		this.assessmentTime = assessmentTime;
	}
	public VehicleMaintenance(int vehicleMaintenanceID,
			String comprehensiveAssessmentID, String busNumber, int phototype,
			String photoPath, int ifAssessment, Date assessmentTime) {
		super();
		this.vehicleMaintenanceID = vehicleMaintenanceID;
		this.comprehensiveAssessmentID = comprehensiveAssessmentID;
		this.busNumber = busNumber;
		this.phototype = phototype;
		this.photoPath = photoPath;
		this.ifAssessment = ifAssessment;
		this.assessmentTime = assessmentTime;
	}
	public int getVehicleMaintenanceID() {
		return vehicleMaintenanceID;
	}
	public void setVehicleMaintenanceID(int vehicleMaintenanceID) {
		this.vehicleMaintenanceID = vehicleMaintenanceID;
	}
	public String getComprehensiveAssessmentID() {
		return comprehensiveAssessmentID;
	}
	public void setComprehensiveAssessmentID(String comprehensiveAssessmentID) {
		this.comprehensiveAssessmentID = comprehensiveAssessmentID;
	}
	public String getBusNumber() {
		return busNumber;
	}
	public void setBusNumber(String busNumber) {
		this.busNumber = busNumber;
	}
	public int getPhototype() {
		return phototype;
	}
	public void setPhototype(int phototype) {
		this.phototype = phototype;
	}
	public String getPhotoPath() {
		return photoPath;
	}
	public void setPhotoPath(String photoPath) {
		this.photoPath = photoPath;
	}
	public int getIfAssessment() {
		return ifAssessment;
	}
	public void setIfAssessment(int ifAssessment) {
		this.ifAssessment = ifAssessment;
	}
	public Date getAssessmentTime() {
		return assessmentTime;
	}
	public void setAssessmentTime(Date assessmentTime) {
		this.assessmentTime = assessmentTime;
	}

}
