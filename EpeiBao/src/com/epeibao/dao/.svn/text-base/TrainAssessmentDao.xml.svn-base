<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.epeibao.dao.TrainAssessmentDao">
	<sql id="Base_Column_List">
		TrainAssessmentID,ComprehensiveAssessmentID,IfTrain,TrainTime,ifAssessment,CauseAnalysis,AssessmentTimes
	</sql>
	
	<select id="findByid" parameterType="java.lang.String" resultType="com.epeibao.po.TrainAssessment">
		select
		<include refid="Base_Column_List" />
		from TrainAssessment
		where ComprehensiveAssessmentID = #{comprehensiveAssessmentID}
		and  AssessmentTimes =(select max(AssessmentTimes) from TrainAssessment ) 
	</select>

</mapper>